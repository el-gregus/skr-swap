# SKR Swap Bot Environment Variables

# Logging
LOG_LEVEL=INFO

# Wallet (DO NOT COMMIT ACTUAL KEYS - USE .env ONLY)
WALLET_PRIVATE_KEY=your_base58_encoded_private_key_here

# Solana RPC (Helius recommended for better rate limits - get free API key at helius.dev)
# Standard public RPC (has rate limits): https://api.mainnet-beta.solana.com
# Helius RPC format: https://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_API_KEY
SOLANA_RPC_URL=https://mainnet.helius-rpc.com/?api-key=your_helius_api_key_here

# Jupiter API
JUPITER_API_URL=https://quote-api.jup.ag/v6
JUPITER_API_KEY=your_jupiter_api_key_here

# Webhook Security
WEBHOOK_SECRET=your_secret_here
REQUIRE_WEBHOOK_SECRET=false

# Dashboard
DASHBOARD_PASSWORD=admin
